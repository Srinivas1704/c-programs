#include<stdio.h>
void main()
{
 int i,n,pro,sum,a[5];
 printf("Enter the n value: ");
 scanf("%d",&n);
 pro=1;
 sum=0;
 printf("Enter the numbers into array:\n");
 for(i=0;i<n;i++)
 {
  scanf("%d",&a[i]);
 }
  for(i=0;i<n;i++)
  {
   pro*=a[i];
  }
  for(i=0;i<n;i++)
  {
   sum+=a[i];
  }
 printf("%d",pro-sum);
}
